#set("content") {
  <h1>#(genre.name)</h1>

  #if(songs) { 
    <table>
      <thead>
        <tr>
          <th>Artist</th>
          <th>Title</th>
        </tr>
      </thead>
      <tbody>
        #for(song in songs) {
          <tr><td>#(song.artist)</td><td><a href="/songs/#(song.id)">#(song.title)</a></td></tr>
        }
      </tbody>
    </table>
  } else {
    <h2>There aren't any song yet!</h2>
  }
}
#embed("base")